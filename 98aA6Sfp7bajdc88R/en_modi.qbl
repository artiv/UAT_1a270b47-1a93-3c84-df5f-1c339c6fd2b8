00\nअपने\n\n711\n00:26:18,300 --> 00:26:19,100\nदायित्वों का\n\n712\n00:26:19,100 --> 00:26:21,200\nइसी तरह निर्वाह\n\n713\n00:26:21,200 --> 00:26:22,400\nकरते रहेंगे।\n\n714\n00:26:22,400 --> 00:26:23,600\nहां\n\n715\n00:26:23,600 --> 00:26:24,800\nमैं मानता\n\n716\n00:26:24,800 --> 00:26:26,900\nकैसे समय में\n\n717\n00:26:26,900 --> 00:26:29,200\nकुछ कठिनाई अभी आती?\n\n718\n00:26:30,600 --> 00:26:31,500\nआशंकाओं\n\n719\n00:26:31,500 --> 00:26:33,900\nवोडाफोन का वातावरण भी पैदा होता है\n\n720\n00:26:33,900 --> 00:26:35,800\nकई बार एक\n\n721\n00:26:35,800 --> 00:26:38,000\nनागरिक के तौर पर\n\n722\n00:26:38,000 --> 00:26:39,800\nहमारी\n\n723\n00:26:39,800 --> 00:26:40,700\nअपेक्षाएं भी\n\n724\n00:26:40,700 --> 00:26:43,400\nपूरी नहीं हो पाती\n\n725\n00:26:43,400 --> 00:26:44,500\nफिर भी\n\n726\n00:26:44,500 --> 00:26:46,500\nयह संकट।\n\n727\n00:26:48,300 --> 00:26:49,700\nइतना बड़ा है और\n\n728\n00:26:49,700 --> 00:26:51,300\nभाई ठीक है।\n\n729\n00:26:51,300 --> 00:26:54,400\nएक भेज भी दूसरे देश को मदद नहीं कर पा रहा।\n\n730\n00:26:56,000 --> 00:26:57,000\nएसपी सिटी में\n\n731\n00:26:57,000 --> 00:26:58,700\nसांवरे\n\n732\n00:26:58,700 --> 00:26:59,700\nदेशवासियों को\n\n733\n00:26:59,700 --> 00:27:01,800\nइन दिक्कतों के भी\n\n734\n00:27:01,800 --> 00:27:04,500\nव्रत संकल्प\n\n735\n00:27:04,500 --> 00:27:05,200\nकथा।\n\n736\n00:27:07,300 --> 00:27:07,700\nइन\n\n737\n00:27:07,700 --> 00:27:08,700\nकठिनाइयों का\n\n738\n00:27:08,700 --> 00:27:11,300\nमुकाबला करने की भी आवश्यकता है।\n\n739\n00:27:12,300 --> 00:27:13,000\nसाथियों।\n\n740\n00:27:14,200 --> 00:27:14,700\nहमें।\n\n741\n00:27:16,000 --> 00:27:16,500\nअभी\n\n742\n00:27:16,500 --> 00:27:17,100\nअपना\n\n743\n00:27:17,100 --> 00:27:18,400\nस्थान आसाम अर्थ।\n\n744\n00:27:19,700 --> 00:27:20,900\nखुद को\n\n745\n00:27:20,900 --> 00:27:21,900\nबचाने के लिए\n\n746\n00:27:21,900 --> 00:27:22,800\nकोरोना से\n\n747\n00:27:22,800 --> 00:27:23,800\nबचने के लिए\n\n748\n00:27:23,800 --> 00:27:24,600\nही\n\n749\n00:27:24,600 --> 00:27:25,900\nप्राथमिकता देनी है।\n\n750\n00:27:27,300 --> 00:27:27,800\nआज।\n\n751\n00:27:29,400 --> 00:27:30,200\nदेश में\n\n752\n00:27:30,200 --> 00:27:32,500\nकेंद्र सरकार हो\n\n753\n00:27:32,500 --> 00:27:34,600\nराज्य सरकारें हो\n\n754\n00:27:34,600 --> 00:27:36,700\nस्थानीय निकाय हो\n\n755\n00:27:36,700 --> 00:27:37,900\nपंचायतें हो।\n\n756\n00:27:39,100 --> 00:27:40,400\nजनप्रतिनिधि हो\n\n757\n00:27:40,400 --> 00:27:42,000\nया फिर सिविल\n\n758\n00:27:42,000 --> 00:27:42,600\nसोसायटी\n\n759\n00:27:42,600 --> 00:27:43,800\nहर कोई\n\n760\n00:27:43,800 --> 00:27:45,200\nअपने अपने\n\n761\n00:27:45,200 --> 00:27:46,200\nतरीके से\n\n762\n00:27:46,200 --> 00:27:47,800\nइस वैश्विक\n\n763\n00:27:47,800 --> 00:27:49,400\nमहामारी से बचने में\n\n764\n00:27:49,400 --> 00:27:51,000\nअपना योगदान\n\n765\n00:27:51,000 --> 00:27:52,200\nदे रहा है।\n\n766\n00:27:52,200 --> 00:27:53,900\nआपको भी\n\n767\n00:27:53,900 --> 00:27:56,000\nअपना पूरा योगदान\n\n768\n00:27:56,000 --> 00:27:57,100\nदेना है।\n\n769\n00:27:57,100 --> 00:27:59,300\nयह आवश्यक है\n\n770\n00:27:59,300 --> 00:28:00,700\nकि वैश्विक\n\n771\n00:28:00,700 --> 00:28:02,600\nमहामारी के इस वातावरण में\n\n772\n00:28:03,700 --> 00:28:05,900\nमानव जाति वीडियो।\n\n773\n00:28:07,400 --> 00:28:08,700\nभारत विजई हो।\n\n774\n00:28:11,300 --> 00:28:12,300\nमेरे प्यारे देशवासियों।\n\n775\n00:28:14,200 --> 00:28:15,000\nकुछ दिन में।\n\n776\n00:28:16,700 --> 00:28:18,700\nनवरात्रि का पर्व आ रहा है।\n\n777\n00:28:20,600 --> 00:28:22,500\nयह शक्ति उपासना का पर्व है\n\n778\n00:28:22,500 --> 00:28:24,300\nभारत\n\n779\n00:28:24,300 --> 00:28:26,400\nपूरी शक्ति के साथ।\n\n780\n00:28:27,800 --> 00:28:28,600\nआगे बढ़े\n\n781\n00:28:28,600 --> 00:28:31,400\nसंकल्प को लेकर के\n\n782\n00:28:31,400 --> 00:28:32,800\nआवश्यक\n\n783\n00:28:32,800 --> 00:28:34,500\nनियम का पालन करते हुए\n\n784\n00:28:34,500 --> 00:28:35,900\nआओ\n\n785\n00:28:35,900 --> 00:28:37,600\nहम भी बचें\n\n786\n00:28:37,600 --> 00:28:39,600\nदेश भी बचाएं\n\n787\n00:28:3